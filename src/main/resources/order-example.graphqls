schema {
    query: Query
    mutation: Mutation
}

type Query {
    order(id: Long): Order
}

type Mutation {
    createOrder(order: Order)
}

type Order {
    totalPrice: String,
    orderItems: [OrderItem]
}

type OrderItem {
    product: Product,
    shipment: Shipment
}

type Product {
    imageUrl: String
}

type Shipment {
    status: String
}

